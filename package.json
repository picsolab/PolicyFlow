{
    "name": "diffusion2017vis",
    "version": "1.0.0",
    "description": "A visualization for policy diffusion network analysis.",
    "scripts": {
        "prestart": "npm run build",
        "start": "app/env/bin/python ./server.py",
        "predev": "npm run clean",
        "dev": "webpack -w",
        "clean": "rimraf ./app/static/",
        "prebuild": "npm run clean",
        "build": "webpack --config config/webpack.prod.js --progress --profile --bail",
        "stop": "ps aux | grep ./server.py | grep -v grep | awk '{ print $2 }' | xargs kill"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Xynoci/diffusion2017vis.git"
    },
    "keywords": [
        "data-visualization",
        "network-analysis"
    ],
    "author": "PICSO Lab",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Xynoci/diffusion2017vis/issues"
    },
    "homepage": "https://github.com/Xynoci/diffusion2017vis#readme",
    "dependencies": {
        "backbone": "^1.3.3",
        "bootstrap": "^3.3.7",
        "bootstrap-select": "^1.12.2",
        "bootstrap-table": "^1.11.2",
        "core-js": "^2.4.1",
        "css-variables-loader": "^2.0.2",
        "d3": "^3.5.16",
        "d3-geo": "^1.6.4",
        "d3-tip": "^0.6.3",
        "hammerjs": "^2.0.8",
        "jquery": "^3.1.1",
        "lodash": "^4.17.4",
        "rxjs": "5.0.1",
        "topojson": "^3.0.0"
    },
    "devDependencies": {
        "babel-core": "^6.25.0",
        "babel-loader": "^7.1.1",
        "babel-preset-env": "^1.6.0",
        "copy-webpack-plugin": "^4.0.1",
        "css-loader": "^0.26.1",
        "extract-text-webpack-plugin": "2.0.0-beta.5",
        "file-loader": "^0.9.0",
        "html-loader": "^0.4.3",
        "html-webpack-plugin": "^2.29.0",
        "null-loader": "^0.1.1",
        "raw-loader": "^0.5.1",
        "rimraf": "^2.5.2",
        "style-loader": "^0.13.1",
        "topojson-server": "^3.0.0",
        "typescript": "~2.0.10",
        "uglifyjs-webpack-plugin": "^0.4.6",
        "webpack": "2.2.1",
        "webpack-dev-server": "2.4.1",
        "webpack-merge": "^3.0.0"
    }
}
