{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "rules": {
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "no-undef": "warn",
        "no-unreachable": "warn",
        "no-unused-vars": "warn",
        "constructor-super": "warn",
        "valid-typeof": "warn"
    },
    "globals": {
        "$": true,
        "jQuery": true,
        "_": true,
        "d3": true,
        "dc": true,
        "crossfilter": true,
        "Backbone": true,
        "d3tip": true,
        "topojson": true,
        "__webpack_public_path__": true
    }
}